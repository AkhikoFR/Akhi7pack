			<property name="DescriptionKey" value="240 BlackPlaceableDesc"/>
			<property name="Meshfile" value="#@modfolder:Resources/240 Black.unity3d?240"/>
			<property name="DropMeshfile" value="Items/Misc/sack_droppedPrefab"/>
			<property name="EconomicValue" value="2000"/>
			<property class="Action1">
		<property name="Class" value="SpawnVehicle"/>
		<property name="Vehicle" value="240 Black"/>
		</property>
