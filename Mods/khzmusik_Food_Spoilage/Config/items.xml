<?xml version="1.0" encoding="UTF-8"?>
<configs>
    <!-- tags must match the tags in the effect_group -->
    <append xpath="/items/item[starts-with(@name, 'food') and not(contains(@name, 'foodCan')) and not(contains(@name, 'Schematic'))]/property[@name='Tags']/@value">,perkMasterChef</append>

    <!--
        Default: 3 items spoil in 7 days, not including location/container adjustments.
        So in containers without a "PreserveBonus", that is 7 items total in 7 days.
    -->
    <append xpath="/items/item[starts-with(@name, 'food') and not(contains(@name, 'foodCan')) and not(contains(@name, 'Schematic'))]">
        <property name="Spoilable" value="true" />
        <property name="ShowQuality" value="false" />
        <property name="QualityTierColor" value="1" />
        <property name="SpoiledItem" value="foodRottingFlesh" />
        <property name="TickPerLoss" value="168" />
        <property name="SpoilageMax" value="1000" />
        <property name="SpoilagePerTick" value="3" />
    </append>
    <append xpath="/items/item[starts-with(@name, 'resourceCrop')]">
        <property name="Spoilable" value="true" />
        <property name="ShowQuality" value="false" />
        <property name="QualityTierColor" value="1" />
        <!-- Removed in A20 but revived for this mod -->
        <property name="SpoiledItem" value="resourceFertilizer" />
        <property name="TickPerLoss" value="168" />
        <property name="SpoilageMax" value="1000" />
        <property name="SpoilagePerTick" value="3" />
    </append>

    <!-- Disable spoilage on items that don't spoil (or are already spoiled) -->
    <remove xpath="/items/item[@name='foodCornMeal']/property[@name='Spoilable']" />
    <remove xpath="/items/item[@name='foodHoney']/property[@name='Spoilable']" />
    <!-- Moldy bread was removed in A20 -->
    <!-- <remove xpath="/items/item[@name='foodMoldyBread']/property[@name='Spoilable']" /> -->
    <remove xpath="/items/item[@name='foodShamSandwich']/property[@name='Spoilable']" />
    <remove xpath="/items/item[@name='foodRottingFlesh']/property[@name='Spoilable']" />
    <remove xpath="/items/item[@name='resourceCropCoffeeBeans']/property[@name='Spoilable']" />
    <remove xpath="/items/item[@name='resourceCropCottonPlant']/property[@name='Spoilable']" />
    <remove xpath="/items/item[@name='resourceCropHopsFlower']/property[@name='Spoilable']" />

    <!-- PLANTS, VEGGIES, BREAD -->

    <!-- Aloe: default -->
    <!-- Blueberries: 10 days -->
    <set xpath="/items/item[@name='foodCropBlueberries']/property[@name='TickPerLoss']/@value">240</set>
    <set xpath="/items/item[@name='foodCropBlueberries']/property[@name='SpoiledItem']/@value">resourceFertilizer</set>
    <!-- Chrysanthemum: 14 days -->
    <set xpath="/items/item[@name='resourceCropChrysanthemumPlant']/property[@name='TickPerLoss']/@value">336</set>
    <!-- Corn: 5 days -->
    <set xpath="/items/item[@name='foodCropCorn']/property[@name='TickPerLoss']/@value">120</set>
    <set xpath="/items/item[@name='foodCropCorn']/property[@name='SpoiledItem']/@value">resourceFertilizer</set>
    <!-- Goldenrod: 10 days -->
    <set xpath="/items/item[@name='resourceCropGoldenrodPlant']/property[@name='TickPerLoss']/@value">240</set>
    <!-- Mushrooms: default -->
    <!-- Pumpkin: 14 days -->
    <set xpath="/items/item[@name='foodCropPumpkin']/property[@name='TickPerLoss']/@value">336</set>
    <set xpath="/items/item[@name='foodCropPumpkin']/property[@name='SpoiledItem']/@value">resourceFertilizer</set>
    <!-- Potato: 14 days -->
    <set xpath="/items/item[@name='foodCropPotato']/property[@name='TickPerLoss']/@value">336</set>
    <set xpath="/items/item[@name='foodCropPotato']/property[@name='SpoiledItem']/@value">resourceFertilizer</set>
    <!-- Yucca fruit (banana yucca): 14 days -->
    <set xpath="/items/item[@name='foodCropYuccaFruit']/property[@name='TickPerLoss']/@value">336</set>
    <set xpath="/items/item[@name='foodCropYuccaFruit']/property[@name='SpoiledItem']/@value">resourceFertilizer</set>
    <!-- Cornbread: 14 days -->
    <set xpath="/items/item[@name='foodCornBread']/property[@name='TickPerLoss']/@value">336</set>
    <set xpath="/items/item[@name='foodCornBread']/property[@name='SpoiledItem']/@value">resourceFertilizer</set>
    <!-- Pumpkin bread: 14 days -->
    <set xpath="/items/item[@name='foodPumpkinBread']/property[@name='TickPerLoss']/@value">336</set>
    <set xpath="/items/item[@name='foodPumpkinBread']/property[@name='SpoiledItem']/@value">resourceFertilizer</set>

    <!-- MEAT AND DAIRY -->

    <!-- Eggs: 1 month - test to see if this is even worth spoilage -->
    <set xpath="/items/item[@name='foodEgg']/property[@name='TickPerLoss']/@value">672</set>
    <!-- Raw meat: 1 day, but only 1 per day (containers affect it more than other food) -->
    <set xpath="/items/item[@name='foodRawMeat']/property[@name='TickPerLoss']/@value">24</set>
    <set xpath="/items/item[@name='foodRawMeat']/property[@name='SpoilagePerTick']/@value">1</set>
    <!-- Cooked meats: 2 days, but only 1 per day (containers affect them more than other food) -->
    <set xpath="/items/item[@name='foodBoiledMeat']/property[@name='TickPerLoss']/@value">48</set>
    <set xpath="/items/item[@name='foodBoiledMeat']/property[@name='SpoilagePerTick']/@value">1</set>
    <set xpath="/items/item[@name='foodCharredMeat']/property[@name='TickPerLoss']/@value">48</set>
    <set xpath="/items/item[@name='foodCharredMeat']/property[@name='SpoilagePerTick']/@value">1</set>
    <set xpath="/items/item[@name='foodGrilledMeat']/property[@name='TickPerLoss']/@value">48</set>
    <set xpath="/items/item[@name='foodGrilledMeat']/property[@name='SpoilagePerTick']/@value">1</set>
    <!-- Stews: 4 days, but only 1 per day (containers affect them more than other food) -->
    <set xpath="/items/item[@name='foodMeatStew']/property[@name='TickPerLoss']/@value">96</set>
    <set xpath="/items/item[@name='foodMeatStew']/property[@name='SpoilagePerTick']/@value">1</set>
