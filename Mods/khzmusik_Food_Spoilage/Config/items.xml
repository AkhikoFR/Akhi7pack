            <property name="CustomIcon" value="drinkJarBeer" />
            <property name="CustomIconTint" value="ccffcc" />
            <property name="UnlockedBy" value="craftingFood" />
            <property name="DisplayType" value="waterBeer" />
            <property name="Group" value="Food/Cooking,CFDrink/Cooking,Medical" />
            <property class="Action0">
                <property name="Class" value="Eat" />
                <property name="Delay" value="2" />
                <property name="Sound_start" value="player_drinking" />
                <!-- <property name="Create_item" value="drinkJarEmpty" /> -->
            </property>
            <effect_group tiered="false" name="Drink Tier 3">
                <requirement name="NotHasBuff" buff="buffIsOnFire" />
                <display_value name="dStaminaRegen" value=".4" />
                <display_value name="dEntityDamageBrawling" value="3" />
                <display_value name="dStunResist" value="1" />
                <display_value name="dDuration" value="60" />
                <triggered_effect trigger="onSelfPrimaryActionEnd" action="ModifyCVar" cvar="$buffBeerDuration" operation="add" value="63" />
                <triggered_effect trigger="onSelfPrimaryActionEnd" action="ModifyCVar" cvar="$buffBeerDuration" operation="set" value="183">
                    <requirement name="CVarCompare" cvar="$buffBeerDuration" operation="GT" value="183" />
                </triggered_effect>
                <triggered_effect trigger="onSelfPrimaryActionEnd" action="AddBuff" buff="buffBeer" />
            </effect_group>

            <effect_group tiered="false">
                <requirement name="HasBuff" buff="buffIsOnFire" />
                <triggered_effect trigger="onSelfPrimaryActionEnd" action="AddBuff" buff="buffExtinguishFire" />
            </effect_group>
        </item>
    </insertAfter>

    <!-- OTHER CONSUMABLES -->

    <!-- Snowballs: 1 hour (24/day), spoils to itself - this gets rid of it entirely -->
