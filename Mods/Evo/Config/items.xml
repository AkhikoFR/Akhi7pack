<configs>

	<append xpath="/items">
	<item name="EvoPlaceable">
			<property name="CustomIcon" value="Evo White logo"/>
			<property name="Extends" value="vehicleMinibikePlaceable"/>
			<property name="Tags" value="vehicle,vengine,vfuel,vlight,vseat,canHaveCosmetic"/> <!-- Choose available mod types -->
			<property name="DescriptionKey" value="EvoPlaceableDesc"/>
			<property name="Meshfile" value="#@modfolder:Resources/Evo Blue.unity3d?Evo"/>
			<property name="DropMeshfile" value="Items/Misc/sack_droppedPrefab"/>
			<property name="EconomicValue" value="2000"/>
			<property class="Action1">
		<property name="Class" value="SpawnVehicle"/>
		<property name="Vehicle" value="Evo"/>
		</property>
			<effect_group name="EvoPlaceable" tiered="false">
				<passive_effect name="ModSlots" operation="base_set" value="4"/>
				<passive_effect name="ModPowerBonus" operation="base_add" value="300" tags="EconomicValue"/>
				<passive_effect name="DegradationMax" operation="base_set" value="7000"/>
			</effect_group>
		</item>



	</append>
</configs>
